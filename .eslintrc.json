{
  "env": {
    "jest/globals": true
  },
  "extends": ["eslint:recommended", "next/core-web-vitals", "prettier"],
  "plugins": ["jest"],
  "rules": {
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": "off",
    "react/no-unescaped-entities": "off",
    "react-hooks/exhaustive-deps": "off"
  }
}
